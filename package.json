		"body-parser": "^1.20.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"moment": "^2.30.1",
		"moment-timezone": "^0.5.45",
		"nodemailer": "^6.9.14",
		"pg": "^8.12.0",
