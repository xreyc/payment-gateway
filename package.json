{
	"name": "payments",
	"version": "1.0.0",
	"description": "Payments API",
	"main": "src/app.js",
	"scripts": {
		"start": "node src/server.js",
		"dev": "nodemon src/server.js",
		"test": "jest",
		"build": "",
		"docker-compose": "sudo docker-compose -f docker-compose.local.yml up",
		"docker-compose-build": "sudo docker-compose -f docker-compose.local.yml up --build",
		"docker-compose-down": "sudo docker-compose down",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
		"db:seed:initial": "npx sequelize-cli db:seed --seed 20240724002826-initial.js",
		"db:seed:testdata": "npx sequelize-cli db:seed --seed 20240831004803-testdata.js"
	},
	"keywords": [
		"payments"
	],
	"author": "Seguma",
	"license": "ISC",
	"dependencies": {
		"@faker-js/faker": "^8.4.1",
